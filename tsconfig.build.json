{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmit": false,
    "strict": true,
    "jsx": "react",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "removeComments": true,
    "rootDir": "./src",
    "baseUrl": ".",
    "declaration": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["src"],
  "exclude": ["node_modules", ".expo", ".expo-shared", "dist"]
}
